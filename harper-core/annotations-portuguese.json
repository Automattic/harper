      "#": "'-(i)(e)s' plural suffix",
      "kind": "suffix",
      "cross_product": true,
      "replacements": [
        {
          "remove": "",
          "add": "s",
          "condition": "[aáeéiíoóuú]"
        },
        {
          "remove": "",
          "add": "es",
          "condition": "[rz]"
        },
        {
          "remove": "l",
          "add": "is",
          "condition": "[l]"
        }
      ],
      "target": [
        {
          "if_base": {
            "noun": {}
          },
          "metadata": {
            "noun": {
              "is_plural": true
            }
          }
        },
        {
          "if_base": {
            "verb": {}
          },
          "metadata": {
            "verb": {
              "verb_form": "THIRD_PERSON_SINGULAR"
            }
          }
        }
      ],
      "base_metadata": {}
    },
    "n": {
      "#": "nominalization suffixes: -ion, -ication, -en",
      "kind": "suffix",
      "cross_product": true,
      "replacements": [
        {
          "remove": "e",
          "add": "ion",
          "condition": "e"
        },
        {
          "remove": "y",
          "add": "ication",
          "condition": "y"
        },
        {
          "remove": "",
          "add": "en",
          "condition": "[^ey]"
        }
      ],
      "target": [],
      "base_metadata": {}
    },
    "^": {
      "#": "'-(i)est' superlative suffix",
      "//": "mnemonic: the topmost is the best. see also 'u' which marks a superlative form",
