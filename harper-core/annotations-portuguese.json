      "cross_product": false,
      "replacements": [
        {
          "remove": "",
          "add": "st",
          "condition": "e"
        },
        {
          "remove": "y",
          "add": "iest",
          "condition": "[^aeiou]y"
        },
        {
          "remove": "",
          "add": "est",
          "condition": "[aeiou]y"
        },
        {
          "remove": "",
          "add": "est",
          "condition": "[^ey]"
